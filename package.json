{
  "name": "paullavenderjones",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/paullj/paullavenderjones.git",
  "author": "Paul Lavender-Jones <paul@lavender-jones.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "studio",
    "web",
    "neat"
  ],
  "scripts": {
    "build": "npm-run-all --parallel build:web build:studio",
    "dev": "npm-run-all --parallel dev:web dev:studio",
    "build:web": "yarn workspace @paullavenderjones/web build",
    "build:studio": "yarn workspace @paullavenderjones/studio build",
    "dev:web": "yarn workspace @paullavenderjones/web dev",
    "dev:studio": "yarn workspace @paullavenderjones/studio start"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}